    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.17",
    "@types/multer": "1.4.7",
    "@types/node": "^20.11.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  }
