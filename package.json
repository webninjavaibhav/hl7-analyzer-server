{
  "name": "hl7-analyzer",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "npm run build"
  },
  "keywords": [
    "diagnostic",
    "hl7",
    "medical",
    "healthcare",
    "api"
